私の持ち物ページ
<h2>口座</h2><br>
<%= form_with(model:@property,local:true) do |f| %>
    <div class="field">
      <%= f.label :body %>
      <%= f.text_area :account, :size => 140 %>
    </div>
    
    <h2>残額</h2><br>
    <div class="field">
    <%= f.label :body %>
    <%= f.text_area :balance, :size => 140 %>
    </div>
  
    <h2>不動産</h2><br>
    <div class="field">
      <%= f.label :body %>
      <%= f.text_area :real_estate, :size => 140 %>
    </div>
  
    <h2>価値</h2><br>
    <div class="field">
      <%= f.label :body %>
      <%= f.text_area :value, :size => 140 %>
    </div>
  
    <h2>保険</h2><br>
    <div class="field">
      <%= f.label :body %>
      <%= f.text_area :insurance, :size => 140 %>
    </div>
    
    <h2>受け取り金額</h2><br>
    <div class="field">
      <%= f.label :body %>
      <%= f.text_area :insurance_proceeds, :size => 140 %>
    </div>
   
    <h2>負債</h2><br>
    <div class="field">
      <%= f.label :body %>
      <%= f.text_area :debt, :size => 140 %>
    </div>
    
    <h2>負債額</h2><br>
    <div class="field">
      <%= f.label :body %>
      <%= f.text_area :debt_amount, :size => 140 %>
    </div>
  


    <%= f.submit "投稿する" %>
  <% end %>

  <%= link_to "財産ページ一覧へ", mylives_path %>
